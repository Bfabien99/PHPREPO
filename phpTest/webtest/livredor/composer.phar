{

$this->request = $this->code = $this->headers = $this->body = null;
}






public static function findHeaderValue(array $headers, $name)
{
$value = null;
foreach ($headers as $header) {
if (Preg::isMatch('{^'.preg_quote($name).':\s*(.+?)\s*$}i', $header, $match)) {
$value = $match[1];
}
}

return $value;
}
