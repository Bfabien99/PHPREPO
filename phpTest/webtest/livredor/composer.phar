protected function execute(InputInterface $input, OutputInterface $output)
{
$config = Factory::createConfig();
$io = $this->getIO();

list($preferSource, $preferDist) = $this->getPreferredInstallOptions($config, $input, true);

if ($input->getOption('dev')) {
